@model CommandSave
@if (Model.UseCancel == true)
{
   var oCancelModel = new CommandBase();
   oCancelModel.Text = FriendCash.Resources.Base.COMMAND_CANCEL;
   oCancelModel.Icon = CommandBase.enIcon.Cancel;
   oCancelModel.Color = CommandBase.enColor.Default;
   if (Model.UsePopup == true) { oCancelModel.Click = "$('#MyPopup').modal('hide');"; }
   else { oCancelModel.Click = "history.back();return false;"; }
   @Html.Partial("Command_Base", oCancelModel)
 }
@Html.Partial("Command_Base", Model)