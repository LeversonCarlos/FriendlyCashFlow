{
   "$schema": "./node_modules/@angular/service-worker/config/schema.json",
   "index": "/index.html",
   "dataGroups": [
      {
         "name": "domainData",
         "urls": [
            "/api/accounts/**",
            "/api/categories/**",
            "/api/recurrencies/types"
         ],
         "cacheConfig": {
            "strategy": "performance",
            "maxSize": 10,
            "maxAge": "15d"
         }
      },
      {
         "name": "dashboardData",
         "urls": [
            "/api/dashboard/**"
         ],
         "cacheConfig": {
            "strategy": "performance",
            "maxSize": 1,
            "maxAge": "1d"
         }
      },
      {
         "name": "transactionData",
         "urls": [
            "/api/entries/**",
            "/api/transfers/**",
            "/api/patterns/**"
         ],
         "cacheConfig": {
            "strategy": "freshness",
            "maxSize": 1,
            "maxAge": "1d",
            "timeout": "10s"
         }
      }
   ],
   "assetGroups": [
      {
         "name": "app",
         "installMode": "prefetch",
         "resources": {
            "files": [
               "/favicon.ico",
               "/index.html",
               "/manifest.json",
               "/*.css",
               "/*.js"
            ]
         }
      },
      {
         "name": "assets",
         "installMode": "lazy",
         "updateMode": "prefetch",
         "resources": {
            "files": [
               "/assets/**",
               "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
            ]
         }
      }
   ]
}
