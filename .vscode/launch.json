{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Attach App",
         "type": "coreclr",
         "request": "attach",
         "processName": "FriendlyCashFlow"
      },
      {
         "name": "Launch App",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceFolder}/srcs/bin/Debug/net6.0/FriendlyCashFlow.dll",
         "args": [],
         "cwd": "${workspaceFolder}/srcs",
         "stopAtEntry": false,
         "env": {
            "ASPNETCORE_ENVIRONMENT": "Development",
            "DEBUG": "true"
         },
         "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
         }
      }
   ]
}
