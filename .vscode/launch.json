{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Attach Backend",
         "type": "coreclr",
         "request": "attach",
         "processName": "Lewio.CashFlow"
      },
      {
         "name": "Launch Backend",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build-backend",
         "program": "${workspaceFolder}/sources/Backend/bin/Debug/net6.0/Lewio.CashFlow.dll",
         "args": [],
         "cwd": "${workspaceFolder}/sources/Backend",
         "stopAtEntry": false,
         "env": {
            "ASPNETCORE_ENVIRONMENT": "Development",
            "DEBUG": "true"
         },
         "sourceFileMap": {
            "/Views": "${workspaceFolder}/sources/Backend/Views"
         }
      },
      {
         "name": "Launch Frontend",
         "type": "msedge",
         "request": "launch",
         "preLaunchTask": "Launch Frontend",
         "url": "http://localhost:4200/"
      }
   ]
}
